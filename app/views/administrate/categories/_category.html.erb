<div id="<%= dom_id category %>">
  <tr>
    <td> <%= l(category.created_at, format: :short) %></td>
    <td> <%= category.name %> </td>
    <td>
      <div class="icons">
        <%= link_to edit_administrate_category_path(category), title: 'Editar' do %>
          <%= image_tag 'icons/gray/edit.svg' %>
        <% end %>
        <%= link_to [:administrate, category], title: 'Visualizar' do %>
          <%= image_tag 'icons/gray/eye.svg' %>
        <% end %>

        <%= button_to [:administrate, category],class: 'button-transparent',alt: 'close', title: 'Apagar',data: {turbo: false}, method: :delete do %>
          <%= image_tag 'icons/gray/close.svg' %>
        <% end %>
      </div>
    </td>
  </tr>
</div>
